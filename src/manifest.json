{
    "name": "TabEx",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "description": "TabEx",
    "icons": {
        "16": "icons/tabex16x16.png",
        "32": "icons/tabex32x32.png",
        "64": "icons/tabex64x64.png",
        "128": "icons/tabex128x128.png"
    },
    "action": {
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "tabs"
    ],
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+Y"
            }
        }
    },
    "options_page": "option/options.html",
    "background": {
        "service_worker": "background.js"
    }
}
